{
  "name": "serverexpress",
  "version": "1.0.0",
  "description": "starting with MERN stack",
  "main": "app.js",
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "scripts": {
    "start": "node app.js",
    "serverName": "nodemon app.js",
    "client": "cd client && npm start --port 3001",
    "dev": "concurrently \"npm run serverName\" \"npm run client \"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Manjunath",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cluster": "^0.7.7",
    "concurrently": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "flash": "^1.1.0",
    "formik": "^1.5.8",
    "fs": "0.0.1-security",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "nodemailer": "^6.3.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pick": "0.0.1",
    "react-hook-form": "^3.20.3",
    "redis": "^2.8.0",
    "uuid": "^3.3.2"
  }
}
